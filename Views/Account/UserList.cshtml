
@model List<Forestitan.Models.UserAccount>

@{
    ViewBag.Title = "User List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>User List</h2>
<br />
<table class="table table-hover">
    <tr>
        <th>User Name</th>
        <th>Email Address</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
        </tr>
    }
</table>